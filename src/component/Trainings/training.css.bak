.checkbox-circle{
   margin-right: 0.6rem;
}
.training-cards{
    display: flex;
    flex-direction: row;
    column-gap: 3rem;
    margin-top: 4rem;
    align-items: center;
    justify-content: center;
    margin-top: 4rem;
}
.card-one{
    background: #FFFFFF;
border: 1px solid #EFF0F7;
box-shadow: 0px 2px 12px rgba(20, 20, 43, 0.08);
border-radius: 24px;
width: 21rem;
}
.card-one:hover{
    background: #106DB3;
    color: #FFFFFF;
    margin-top: -4rem;
}
.section-two{
	display: flex;
	flex-direction: row;
	column-gap: 3rem;
	margin-left: 5rem;
	margin-right: 4rem;
