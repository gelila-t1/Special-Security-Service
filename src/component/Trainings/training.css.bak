/* color: #6F6C90; */
}
.card-one p{
    margin-left: 2rem; 
    margin-right: 2rem;
}
.usd{
    font-family: 'DM Sans';
font-style: normal;
font-weight: 700;
font-size: 54px;
line-height: 66px;
/* color: #170F49; */
